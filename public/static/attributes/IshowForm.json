{
  "classId": "idm.component.mobile.ishowform",
  "comName": "数据展示表单",
  "className": "IshowForm",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
    {
      "type": "input",
      "layoutType": "inline",
      "text": "唯一标识",
      "bindKey": "ctrlId",
      "disabled": true,
      "default": "@[packageid]",
      "desc": "",
      "helpUrl": ""
    },
    {
      "type": "group",
      "text": "基本属性",
      "desc": "提供设置组件能达到展示效果的基本属性设置信息",
      "children": [
        {
          "type": "radio",
          "layoutType": "inline",
          "text": "标题展示",
          "desc": "是否展示标题展示",
          "bindKey": "showTitle",
          "dictionary": [
              {
                  "label": "显示",
                  "value": true
              },
              {
                  "label": "隐藏",
                  "value": false
              }
          ],
          "default": true
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "标题文字",
          "bindKey": "titleContent",
          "default": "第一次党员大会",
          "display": "@[showTitle]"
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "绑定字段",
          "bindKey": "titleField",
          "placeholder": "标题绑定字段",
          "default": "title"
        },
        {
          "type": "radio",
          "layoutType": "inline",
          "text": "列表展示",
          "desc": "列表展示是否展示",
          "bindKey": "showUl",
          "dictionary": [
              {
                  "label": "显示",
                  "value": true
              },
              {
                  "label": "隐藏",
                  "value": false
              }
          ],
          "default": true
        },
        {
          "type": "radio",
          "layoutType": "inline",
          "text": "信息标题",
          "desc": "信息标题是否展示",
          "bindKey": "showInfo",
          "dictionary": [
              {
                  "label": "显示",
                  "value": true
              },
              {
                  "label": "隐藏",
                  "value": false
              }
          ],
          "default": true
        },
        {
          "type": "table",
          "text": "头部列表选项设置",
          "bindKey": "tableList",
          "tabText": "页签[index]",
          "children": [
            {
              "type": "input",
              "layoutType": "inline",
              "text": "tab标识",
              "bindKey": "key"
            },
            {
              "type": "input",
              "layoutType": "inline",
              "text": "tab文字",
              "bindKey": "label"
            },
            {
              "type": "input",
              "layoutType": "inline",
              "text": "绑定字段",
              "bindKey": "field",
              "placeholder": "绑定字段"
            }
          ],
          "default": [
            {
              "key": "1",
              "label": "应到",
              "field": "should"
            },
            {
              "key": "2",
              "label": "实到",
              "field": "actual"
            },
            {
              "key": "3",
              "label": "请假",
              "field": "leave"
            },
            {
              "key": "4",
              "label": "缺席",
              "field": "absent"
            }
          ]
        }
      ]
    },
    {
      "type": "group",
      "text": "外层容器样式",
      "desc": "点击？查看样式设置用法指南",
      "helpUrl": "",
      "children": [
        {
          "type": "box",
          "layoutType": "block",
          "text": "内外边距",
          "bindKey": "box",
          "default": {
            "marginTopVal": "",
            "marginRightVal": "",
            "marginBottomVal": "",
            "marginLeftVal": "",
            "paddingTopVal": "10px",
            "paddingRightVal": "10px",
            "paddingBottomVal": "10px",
            "paddingLeftVal": "10px"
          }
        },
        {
          "type": "colorPicker",
          "layoutType": "inline",
          "text": "背景颜色",
          "bindKey": "boxBg",
          "default": {
            "hex": "#FFFFFF",
            "hex8": "#FFFFFFFF"
          }
        }
      ]
    },
    {
      "type": "group",
      "text": "顶部容器样式",
      "desc": "点击？查看样式设置用法指南",
      "helpUrl": "",
      "children": [
        {
          "type": "input",
          "layoutType": "inline",
          "text": "阴影样式",
          "bindKey": "topShadow",
          "default": "0px 0px 20px 0px #ccc"
        },
        {
          "type": "border",
          "layoutType": "block",
          "text": "边框",
          "bindKey": "topBorder"
        },
        {
          "type": "group",
          "text": "标题样式",
          "desc": "点击？查看样式设置用法指南",
          "helpUrl": "",
          "children": [
            {
              "type": "box",
              "layoutType": "block",
              "text": "内外边距",
              "bindKey": "titleBox",
              "default": {
                "marginTopVal": "",
                "marginRightVal": "20px",
                "marginBottomVal": "",
                "marginLeftVal": "20px",
                "paddingTopVal": "20px",
                "paddingRightVal": "",
                "paddingBottomVal": "20px",
                "paddingLeftVal": ""
              }
            },
            {
              "type": "border",
              "layoutType": "block",
              "text": "底边框",
              "bindKey": "titleBorder"
            },
            {
              "type": "font",
              "layoutType": "block",
              "text": "标题字体",
              "bindKey": "titleFont"
            },
            {
              "type": "iconSelect",
              "layoutType": "inline",
              "text": "自定义图标",
              "bindKey": "titleIcon"
            },
            {
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "图标颜色",
              "bindKey": "titleIconColor"
            },
            {
              "type": "inputNumber",
              "layoutType": "inline",
              "text": "图标大小",
              "bindKey": "titleIconSize",
              "default": 20
            }
          ]
        },
        {
          "type": "group",
          "text": "列表样式",
          "desc": "点击？查看样式设置用法指南",
          "helpUrl": "",
          "children": [
            {
              "type": "box",
              "layoutType": "block",
              "text": "内外边距",
              "bindKey": "ulBox",
              "default": {
                "marginTopVal": "",
                "marginRightVal": "",
                "marginBottomVal": "",
                "marginLeftVal": "",
                "paddingTopVal": "20px",
                "paddingRightVal": "",
                "paddingBottomVal": "20px",
                "paddingLeftVal": ""
              }
            },
            {
              "type": "font",
              "layoutType": "block",
              "text": "数字字体",
              "bindKey": "numFont"
            },
            {
              "type": "font",
              "layoutType": "block",
              "text": "名称字体",
              "bindKey": "nameFont"
            }
          ]
        }
      ]
    },
    {
      "type": "group",
      "text": "基本信息样式",
      "desc": "点击？查看样式设置用法指南",
      "helpUrl": "",
      "children": [
        {
          "type": "input",
          "layoutType": "inline",
          "text": "标题",
          "bindKey": "labelTitle",
          "default": "基本信息"
        },
        {
          "type": "box",
          "layoutType": "block",
          "text": "内外边距",
          "bindKey": "labelBox",
          "default": {
            "marginTopVal": "",
            "marginRightVal": "",
            "marginBottomVal": "",
            "marginLeftVal": "",
            "paddingTopVal": "10px",
            "paddingRightVal": "",
            "paddingBottomVal": "10px",
            "paddingLeftVal": ""
          }
        },
        {
          "type": "font",
          "layoutType": "block",
          "text": "字体",
          "bindKey": "labelFont"
        }
      ]
    },
    {
      "type": "group",
      "text": "底部容器样式",
      "desc": "点击？查看样式设置用法指南",
      "helpUrl": "",
      "children": [
        {
          "type": "box",
          "layoutType": "block",
          "text": "内外边距",
          "bindKey": "contentBox",
          "default": {
            "marginTopVal": "",
            "marginRightVal": "",
            "marginBottomVal": "",
            "marginLeftVal": "",
            "paddingTopVal": "",
            "paddingRightVal": "20px",
            "paddingBottomVal": "20px",
            "paddingLeftVal": "20px"
          }
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "阴影样式",
          "bindKey": "contentShadow",
          "default": "0px 0px 20px 0px #ccc"
        },
        {
          "type": "border",
          "layoutType": "block",
          "text": "边框",
          "bindKey": "contentBorder"
        },
        {
          "type": "group",
          "text": "每个form样式",
          "desc": "点击？查看样式设置用法指南",
          "helpUrl": "",
          "children": [
            {
              "type": "box",
              "layoutType": "block",
              "text": "内外边距",
              "bindKey": "formlabel",
              "default": {
                "marginTopVal": "",
                "marginRightVal": "",
                "marginBottomVal": "",
                "marginLeftVal": "",
                "paddingTopVal": "20px",
                "paddingRightVal": "",
                "paddingBottomVal": "20px",
                "paddingLeftVal": ""
              }
            },
            {
              "type": "border",
              "layoutType": "block",
              "text": "边框",
              "bindKey": "formlabelBorder"
            },
            {
              "type": "font",
              "layoutType": "block",
              "text": "左侧字体",
              "bindKey": "leftTitleFont"
            },
            {
              "type": "font",
              "layoutType": "block",
              "text": "右侧字体",
              "bindKey": "rightTitleFont"
            },
            {
              "type": "font",
              "layoutType": "block",
              "text": "相关资料字体",
              "bindKey": "meansfont"
            },
            {
              "type": "box",
              "layoutType": "block",
              "text": "相关资料间距",
              "bindKey": "meansMargin",
              "default": {
                "marginTopVal": "",
                "marginRightVal": "",
                "marginBottomVal": "",
                "marginLeftVal": "",
                "paddingTopVal": "",
                "paddingRightVal": "",
                "paddingBottomVal": "10px",
                "paddingLeftVal": ""
              }
            }
          ]
        }
      ]
    },
    {
      "type": "table",
      "text": "主题设置",
      "desc": "点击？查看主题设置用法指南",
      "bindKey": "themeList",
      "tabText": "主题[index]",
      "default":[
          {
              "key":"blue",
              "mainColor":{
                  "hex": "#0073CA",
                  "hex8": "#0073CAFF"
              }
          },
          {
              "key":"red",
              "mainColor":{
                  "hex": "#E21A1A",
                  "hex8": "#E21A1AFF"
              }
          },
          {
              "key":"green",
              "mainColor":{
                  "hex": "#0EAF64",
                  "hex8": "#0EAF64FF"
              }
          }
      ],
      "children": [
          {
              "type": "select",
              "layoutType": "inline",
              "text": "主题标识",
              "bindKey": "key",
              "multiple": false,
              "desc":"用于设置主题的标识，此项需要搭配自定义开发切换主题的功能，例如：蓝色标识为blue，用户切换主题的时候存储到数据库中的标识为blue，则这里填写为blue",
              "default":"",
              "placeholder":"例如：blue",
              "ctrlAttrObject": {
                  "mode": "combobox"
              },
              "dictionary": [
                  {
                      "label": "blue",
                      "value": "blue"
                  },
                  {
                      "label": "red",
                      "value": "red"
                  },
                  {
                      "label": "green",
                      "value": "green"
                  }
              ]
          },{
              "type": "colorPicker",
              "layoutType": "inline",
              "text": "主要颜色",
              "bindKey": "mainColor",
              "desc":"主题的主要颜色",
              "placeholder": "请选择颜色",
              "display":"@[key]"
          }
      ]
    },
    {
      "type": "group",
      "text": "高级",
      "desc": "点击？查看高级设置用法指南",
      "children": [
        {
          "type": "treeSelect",
          "layoutType": "inline",
          "text": "数据源",
          "bindKey": "dataSource",
          "dataSourceUrl": "/ctrl/dataSource/getDatasourceByGroup"
        },
        {
          "type": "actionSelect",
          "layoutType": "block",
          "text": "接口参数自定义函数",
          "bindKey": "customClickFunc",
          "desc": "接口参数自定义函数",
          "multiple": false
        },
        {
          "type": "group",
          "text": "字段设置",
          "desc": "设置从列表项中展示的对应字段",
          "children": [
            {
              "type": "input",
              "layoutType": "inline",
              "text": "地址字段",
              "bindKey": "addressField",
              "default": "address"
            },
            {
              "type": "input",
              "layoutType": "inline",
              "text": "时间字段",
              "bindKey": "timeField",
              "default": "time"
            },
            {
              "type": "input",
              "layoutType": "inline",
              "text": "联系人字段",
              "bindKey": "peopleField",
              "default": "people"
            },
            {
              "type": "input",
              "layoutType": "inline",
              "text": "活动内容字段",
              "bindKey": "activeField",
              "default": "active"
            },
            {
              "type": "input",
              "layoutType": "inline",
              "text": "资料字段",
              "bindKey": "infoField",
              "default": "info"
            },
            {
              "type": "input",
              "layoutType": "inline",
              "text": "活动总结字段",
              "bindKey": "activeSumField",
              "default": "activeSum"
            }
          ]
        }
      ]
    }
  ]
}