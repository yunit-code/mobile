{
  "classId": "idm.component.mobile.itags",
  "comName": "标签选择器",
  "className": "ITags",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
      {
        "type": "input",
        "layoutType": "inline",
        "text": "唯一标识",
        "bindKey": "ctrlId",
        "disabled": true,
        "default": "@[packageid]",
        "desc": "",
        "helpUrl": ""
      },
      {
        "type": "group",
        "text": "基本属性",
        "desc": "提供设置组件能达到展示效果的基本属性设置信息",
        "children": [
          {
            "type": "switch",
            "layoutType": "inline",
            "text": "显示标题",
            "desc":"个人页签是否显示",
            "bindKey": "selfShow",
            "default": true
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "标题内容",
            "bindKey": "title",
            "default": "类型选择(多选)"
          },
          {
            "type": "radio",
            "layoutType": "inline",
            "text": "页签来源",
            "desc":"用于设置页签数据来源，动态属性：使用控制中心设置返回的数据源（支持用户个性化定制和排序），静态属性：直接通过属性设置数据源，无法实现定制和排序",
            "bindKey": "tabDataType",
            "default": "static",
            "dictionary": [
              {
                "label": "动态属性",
                "value": "dynamic"
              },
              {
                "label": "静态属性",
                "value": "static"
              }
            ]
          },
          {
            "type": "table",
            "text": "页签选项设置",
            "bindKey": "staticTabPaneList",
            "tabText": "页签[index]",
            "display":"@[tabDataType=='static']",
            "children": [
              {
                "type": "input",
                "layoutType": "inline",
                "text": "tab标识",
                "bindKey": "key"
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "tab文字",
                "bindKey": "tab"
              },
              {
                "type": "switch",
                "layoutType": "inline",
                "text": "默认选中",
                "bindKey": "defaultActiveKey",
                "default": false
              }
            ],
            "default": [
              {
                "key": 1,
                "tab": "组织生活"
              },
              {
                "key": 2,
                "tab": "党费",
                "defaultActiveKey": true
              }
            ]
          }
        ]
      },
      {
        "type": "group",
        "text": "外层容器样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "inlineGroup",
            "text": "容器",
            "children": [
                {
                    "type": "input",
                    "layoutType": "block",
                    "text": "宽",
                    "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                    "bindKey": "boxWidth",
                    "width": "100%",
                    "default": "auto"
                },
                {
                    "type": "input",
                    "layoutType": "block",
                    "text": "高",
                    "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                    "bindKey": "boxHeight",
                    "width": "200px",
                    "default": "auto"
                }
            ]
          },
          {
            "type": "colorPicker",
            "layoutType": "inline",
            "desc": "背景颜色",
            "text": "背景颜色",
            "bindKey": "boxbg"
          },
          {
            "type": "box",
            "layoutType": "block",
            "text": "内外边距",
            "bindKey": "box",
            "default": {
                "marginTopVal": "",
                "marginRightVal": "",
                "marginBottomVal": "",
                "marginLeftVal": "",
                "paddingTopVal": "",
                "paddingRightVal": "",
                "paddingBottomVal": "",
                "paddingLeftVal": ""
            }
          }
        ]
      },
      {
        "type": "group",
        "text": "内层容器样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "font",
            "layoutType": "block",
            "text": "标题样式",
            "bindKey": "itemFontStyle",
            "default": {
                "fontColors": {
                    "hex": "#666"
                },
                "fontSize": 14,
                "fontSizeUnit": "px"
            }
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "最小宽度",
            "bindKey": "minWidth",
            "default": "80px"
          },
          {
            "type": "box",
            "layoutType": "block",
            "text": "标签边距",
            "bindKey": "tagBox",
            "default": {
                "marginTopVal": "10px",
                "marginRightVal": "10px",
                "marginBottomVal": "",
                "marginLeftVal": "",
                "paddingTopVal": "5px",
                "paddingRightVal": "",
                "paddingBottomVal": "5px",
                "paddingLeftVal": ""
            }
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "默认字体",
            "bindKey": "tagStyle",
            "default": {
              "fontColors": {
                  "hex": "#666"
              },
              "fontSize": 14,
              "fontSizeUnit": "px"
            }
          },
          {
            "type": "border",
            "layoutType": "block",
            "text": "默认字体边框",
            "bindKey": "border",
            "default": {
              "border": {
                "bottom": {
                  "colors": {
                    "hex": "#D4CACA",
                    "hex8": "#D4CACAFF"
                  },
                  "style": "solid",
                  "width": 1,
                  "widthUnit": "px"
                },
                "left": {
                  "colors": {
                    "hex": "#D4CACA",
                    "hex8": "#D4CACAFF"
                  },
                  "style": "solid",
                  "width": 1,
                  "widthUnit": "px"
                },
                "right": {
                  "colors": {
                    "hex": "#D4CACA",
                    "hex8": "#D4CACAFF"
                  },
                  "style": "solid",
                  "width": 1,
                  "widthUnit": "px"
                },
                "top": {
                  "colors": {
                    "hex": "#D4CACA",
                    "hex8": "#D4CACAFF"
                  },
                  "style": "solid",
                  "width": 1,
                  "widthUnit": "px"
                }
              },
              "radius": {
                "leftBottom": {
                  "radius": 30,
                  "radiusUnit": "px"
                },
                "leftTop": {
                  "radius": 30,
                  "radiusUnit": "px"
                },
                "rightBottom": {
                  "radius": 30,
                  "radiusUnit": "px"
                },
                "rightTop": {
                  "radius": 30,
                  "radiusUnit": "px"
                }
              }
            }
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "选中字体",
            "bindKey": "tagActiveStyle",
            "default": {
                "fontColors": {
                    "hex": "#666"
                },
                "fontSize": 14,
                "fontSizeUnit": "px"
            }
          },
          {
            "type": "border",
            "layoutType": "block",
            "text": "选中边框",
            "bindKey": "tagActiveBorder",
            "default": {
              "border": {
                "bottom": {
                  "colors": {},
                  "style": "solid",
                  "width": 1,
                  "widthUnit": "px"
                },
                "left": {
                  "colors": {},
                  "style": "solid",
                  "width": 1,
                  "widthUnit": "px"
                },
                "right": {
                  "colors": {},
                  "style": "solid",
                  "width": 1,
                  "widthUnit": "px"
                },
                "top": {
                  "colors": {},
                  "style": "solid",
                  "width": 1,
                  "widthUnit": "px"
                }
              },
              "radius": {
                "leftBottom": {
                  "radius": 30,
                  "radiusUnit": "px"
                },
                "leftTop": {
                  "radius": 30,
                  "radiusUnit": "px"
                },
                "rightBottom": {
                  "radius": 30,
                  "radiusUnit": "px"
                },
                "rightTop": {
                  "radius": 30,
                  "radiusUnit": "px"
                }
              }
            }
          }
        ]
      },
      {
        "type": "table",
        "text": "主题设置",
        "desc": "点击？查看主题设置用法指南",
        "bindKey": "themeList",
        "tabText": "主题[index]",
        "default":[
            {
                "key":"blue",
                "mainColor":{
                    "hex": "#0073CA",
                    "hex8": "#0073CAFF"
                }
            },
            {
                "key":"red",
                "mainColor":{
                    "hex": "#E21A1A",
                    "hex8": "#E21A1AFF"
                }
            },
            {
                "key":"green",
                "mainColor":{
                    "hex": "#0EAF64",
                    "hex8": "#0EAF64FF"
                }
            }
        ],
        "children": [
            {
                "type": "select",
                "layoutType": "inline",
                "text": "主题标识",
                "bindKey": "key",
                "multiple": false,
                "desc":"用于设置主题的标识，此项需要搭配自定义开发切换主题的功能，例如：蓝色标识为blue，用户切换主题的时候存储到数据库中的标识为blue，则这里填写为blue",
                "default":"",
                "placeholder":"例如：blue",
                "ctrlAttrObject": {
                    "mode": "combobox"
                },
                "dictionary": [
                    {
                        "label": "blue",
                        "value": "blue"
                    },
                    {
                        "label": "red",
                        "value": "red"
                    },
                    {
                        "label": "green",
                        "value": "green"
                    }
                ]
            },{
                "type": "colorPicker",
                "layoutType": "inline",
                "text": "主要颜色",
                "bindKey": "mainColor",
                "desc":"主题的主要颜色",
                "placeholder": "请选择颜色",
                "display":"@[key]"
            }
        ]
      },
      {
        "type": "group",
        "text": "高级",
        "desc": "点击？查看高级设置用法指南",
        "children": [
          {
            "type": "dataSourceSelect",
            "layoutType": "inline",
            "text": "数据源",
            "desc": "用于选择数据源",
            "bindKey": "dataSource"
          },
          {
            "type": "pageModuleSelect",
            "layoutType": "block",
            "text": "tag菜单联动组件发送消息",
            "desc": "选择点击tag需要联动的组件",
            "bindKey": "triggerComponents",
            "multiple": true
          },
          {
            "type": "group",
            "text": "字段设置",
            "desc": "设置从列表项中展示的对应字段",
            "children": [
              {
                "type": "dataSourceFiledSelect",
                "layoutType": "inline",
                "text": "名称字段",
                "bindKey": "nameField",
                "default": "name"
              }
            ]
          }
        ]
      }
  ]
}
    