{
  "classId": "idm.component.mobile.icommentpublish",
  "comName": "评论编辑",
  "className": "ICommentPublish",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
    {
      "type": "input",
      "layoutType": "inline",
      "text": "唯一标识",
      "bindKey": "ctrlId",
      "disabled": true,
      "default": "@[packageid]",
      "desc": "",
      "helpUrl": ""
    },
    {
      "type": "group",
      "text": "高级",
      "desc": "点击？查看高级设置用法指南",
      "children": [
        {
          "type": "actionSelect",
          "layoutType": "block",
          "text": "设置请求评论参数",
          "desc": "设置请求评论参数",
          "bindKey": "publishComment",
          "multiple": false
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "提交接口",
          "bindKey": "publishCommentUrl"
        },
        {
          "type": "pageModuleSelect",
          "layoutType": "block",
          "text": "评论菜单联动组件",
          "desc": "选择点击评论菜单需要联动组件",
          "bindKey": "triggerComponents",
          "multiple": true
        }
      ]
    }
  ]
}
