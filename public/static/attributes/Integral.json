{
  "classId": "idm.component.mobile.integral",
  "comName": "我的积分",
  "className": "Integral",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
      {
        "type": "input",
        "layoutType": "inline",
        "text": "唯一标识",
        "bindKey": "ctrlId",
        "disabled": true,
        "default": "@[packageid]",
        "desc": "",
        "helpUrl": ""
      },
      {
        "type": "group",
        "text": "头部容器样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "inlineGroup",
            "text": "宽高",
            "children": [
              {
                "type": "input",
                "layoutType": "block",
                "text": "宽",
                "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                "bindKey": "imgWidth",
                "width": "60px",
                "default": "auto"
              },
              {
                "type": "input",
                "layoutType": "block",
                "text": "高",
                "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                "bindKey": "imgHeight",
                "width": "60px",
                "default": "200px"
              }
            ]
          },
          {
            "type": "switch",
            "layoutType": "inline",
            "text": "图片显示",
            "desc":"图片是否显示",
            "bindKey": "showImg",
            "default": true
          },
          {
            "type": "uploadImage",
            "layoutType": "inline",
            "text": "图片地址",
            "bindKey": "topImgUrl",
            "placeholder": "可输入图片地址或直接上传"
          },
          {
            "type": "colorPicker",
            "layoutType": "inline",
            "text": "背景色",
            "bindKey": "colorBg",
            "placeholder": "请选择颜色"
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "累计积分",
            "bindKey": "titleFont"
          },
          {
            "type": "group",
            "text": "积分定位",
            "desc": "积分定位",
            "helpUrl": "",
            "children": [
              {
                "type": "input",
                "layoutType": "inline",
                "text": "积分定位top",
                "desc": "积分距离top定位",
                "bindKey": "boxposition",
                "default": "35%"
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "总分数定位top",
                "desc": "总分数距离top定位",
                "bindKey": "allboxPostion",
                "default": "35px"
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "已获得分数top定位",
                "desc": "已获得分数top定位",
                "bindKey": "getBoxPostionTop",
                "default": "15px"
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "已获得分数right定位",
                "desc": "已获得分数right定位",
                "bindKey": "getBoxPostionRight",
                "default": "-123px"
              }
            ]
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "积分",
            "bindKey": "subTitleFont"
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "描述积分",
            "bindKey": "descTitleFont"
          }
        ]
      },
      {
        "type": "group",
        "text": "底部容器样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "colorPicker",
            "layoutType": "inline",
            "text": "背景色",
            "bindKey": "bgColor",
            "placeholder": "请选择颜色",
            "default": {
              "hex": "#FFFFFFFF",
              "hex8": "#FFFFFFFF"
            }
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "容器阴影",
            "bindKey": "boxShadow",
            "default": "0px 0px 10px 0px #e3dede"
          },
          {
            "type": "border",
            "layoutType": "block",
            "text": "容器边框",
            "bindKey": "boxBorder"
          },
          {
            "type": "box",
            "layoutType": "block",
            "text": "内外边距",
            "bindKey": "box",
            "default": {
              "marginTopVal": "-20px",
              "marginRightVal": "10px",
              "marginBottomVal": "",
              "marginLeftVal": "10px",
              "paddingTopVal": "",
              "paddingRightVal": "10px",
              "paddingBottomVal": "",
              "paddingLeftVal": "10px"
            }
          },
          {
            "type": "group",
            "text": "积分奖励样式",
            "desc": "点击？查看样式设置用法指南",
            "helpUrl": "",
            "children": [
              {
                "type": "switch",
                "layoutType": "inline",
                "text": "标题显示",
                "desc":"积分奖励是否显示",
                "bindKey": "integralShow",
                "default": true
              },
              {
                "type": "iconSelect",
                "layoutType": "inline",
                "text": "自定义图标",
                "bindKey": "boxtitleIcon"
              },
              {
                "type": "colorPicker",
                "layoutType": "inline",
                "text": "图标颜色",
                "bindKey": "titleIconColor"
              },
              {
                "type": "inputNumber",
                "layoutType": "inline",
                "text": "图标大小",
                "bindKey": "titleIconSize",
                "default": 20
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "积分奖励",
                "bindKey": "integralTitle",
                "default": "积分奖励"
              },
              {
                "type": "font",
                "layoutType": "block",
                "text": "积分字体",
                "bindKey": "integralFont"
              }
            ]
          },
          {
            "type": "box",
            "layoutType": "block",
            "text": "列表边距",
            "bindKey": "liBox",
            "default": {
              "marginTopVal": "",
              "marginRightVal": "",
              "marginBottomVal": "",
              "marginLeftVal": "",
              "paddingTopVal": "10px",
              "paddingRightVal": "",
              "paddingBottomVal": "10px",
              "paddingLeftVal": ""
            }
          },
          {
            "type": "border",
            "layoutType": "block",
            "text": "列表边框",
            "bindKey": "liboxBorder"
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "标题字体",
            "bindKey": "titleLiFont"
          },
          {
            "type": "box",
            "layoutType": "block",
            "text": "按钮边距",
            "bindKey": "btnMargin",
            "default": {
              "marginTopVal": "",
              "marginRightVal": "",
              "marginBottomVal": "",
              "marginLeftVal": "",
              "paddingTopVal": "3px",
              "paddingRightVal": "2px",
              "paddingBottomVal": "3px",
              "paddingLeftVal": "2px"
            }
          },
          {
            "type": "border",
            "layoutType": "block",
            "text": "按钮边框",
            "bindKey": "btnBorder"
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "按钮字体",
            "bindKey": "btnFont"
          },
          {
            "type": "table",
            "text": "按钮字体设置",
            "bindKey": "btnTable",
            "tabText": "按钮[index]",
            "children": [
              {
                "type": "input",
                "layoutType": "inline",
                "text": "按钮名称",
                "bindKey": "name"
              },
              {
                "type": "colorPicker",
                "layoutType": "inline",
                "text": "背景色",
                "bindKey": "btnBg",
                "placeholder": "请选择颜色"
              }
            ],
            "default": [
              {
                "name": "已完成",
                "btnBg": {
                  "hex": "#D2D2D2",
                  "hex8": "#D2D2D2FF"
                }
              },
              {
                "name": "去看看",
                "btnBg": {
                  "hex": "#CD0502",
                  "hex8": "#CD0502FF"
                }
              }
            ]
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "描述字体",
            "bindKey": "descFont"
          },
          {
            "type": "box",
            "layoutType": "block",
            "text": "描述边距",
            "bindKey": "descBox",
            "default": {
              "marginTopVal": "",
              "marginRightVal": "",
              "marginBottomVal": "",
              "marginLeftVal": "",
              "paddingTopVal": "5px",
              "paddingRightVal": "",
              "paddingBottomVal": "",
              "paddingLeftVal": ""
            }
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "提示字体",
            "bindKey": "tipsFont"
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "提示距离",
            "bindKey": "tipsBox",
            "default": "5px"
          }
        ]
      },
      {
        "type": "table",
        "text": "主题设置",
        "desc": "点击？查看主题设置用法指南",
        "bindKey": "themeList",
        "tabText": "主题[index]",
        "default":[
            {
                "key":"blue",
                "mainColor":{
                    "hex": "#0073CA",
                    "hex8": "#0073CAFF"
                }
            },
            {
                "key":"red",
                "mainColor":{
                    "hex": "#E21A1A",
                    "hex8": "#E21A1AFF"
                }
            },
            {
                "key":"green",
                "mainColor":{
                    "hex": "#0EAF64",
                    "hex8": "#0EAF64FF"
                }
            }
        ],
        "children": [
            {
                "type": "select",
                "layoutType": "inline",
                "text": "主题标识",
                "bindKey": "key",
                "multiple": false,
                "desc":"用于设置主题的标识，此项需要搭配自定义开发切换主题的功能，例如：蓝色标识为blue，用户切换主题的时候存储到数据库中的标识为blue，则这里填写为blue",
                "default":"",
                "placeholder":"例如：blue",
                "ctrlAttrObject": {
                    "mode": "combobox"
                },
                "dictionary": [
                    {
                        "label": "blue",
                        "value": "blue"
                    },
                    {
                        "label": "red",
                        "value": "red"
                    },
                    {
                        "label": "green",
                        "value": "green"
                    }
                ]
            },{
                "type": "colorPicker",
                "layoutType": "inline",
                "text": "主要颜色",
                "bindKey": "mainColor",
                "desc":"主题的主要颜色",
                "placeholder": "请选择颜色",
                "display":"@[key]"
            }
        ]
      },
      {
        "type": "group",
        "text": "高级",
        "desc": "点击？查看高级设置用法指南",
        "children": [
          {
            "type": "treeSelect",
            "layoutType": "inline",
            "text": "数据源",
            "bindKey": "dataSource",
            "dataSourceUrl":"/ctrl/dataSource/getDatasourceByGroup"
          },
          {
            "type": "group",
            "text": "字段设置",
            "desc": "页面数据绑定",
            "children": [
              {
                "type": "input",
                "layoutType": "inline",
                "text": "总积分",
                "bindKey": "allField",
                "default": "allData"
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "今日积分",
                "bindKey": "dateInteField",
                "default": "dateintegral"
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "列表字段",
                "bindKey": "liFiled",
                "default": "list"
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "标题字段",
                "bindKey": "titleFiled",
                "default": "name"
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "描述字段",
                "bindKey": "descFiled",
                "default": "desc"
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "类型字段",
                "bindKey": "typeFiled",
                "default": "type"
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "获得积分",
                "bindKey": "gotFiled",
                "default": "got"
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "上限积分",
                "bindKey": "onlineFiled",
                "default": "online"
              }
            ]
          }
        ]
      }
  ]
}