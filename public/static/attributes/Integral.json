{
  "classId": "idm.component.mobile.integral",
  "comName": "我的积分",
  "className": "Integral",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
      {
        "type": "input",
        "layoutType": "inline",
        "text": "唯一标识",
        "bindKey": "ctrlId",
        "disabled": true,
        "default": "@[packageid]",
        "desc": "",
        "helpUrl": ""
      },
      {
        "type": "group",
        "text": "头部容器样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "inlineGroup",
            "text": "宽高",
            "children": [
              {
                "type": "input",
                "layoutType": "block",
                "text": "宽",
                "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                "bindKey": "imgWidth",
                "width": "60px",
                "default": "auto"
              },
              {
                "type": "input",
                "layoutType": "block",
                "text": "高",
                "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                "bindKey": "imgHeight",
                "width": "60px",
                "default": "200px"
              }
            ]
          },
          {
            "type": "switch",
            "layoutType": "inline",
            "text": "图片显示",
            "desc":"图片是否显示",
            "bindKey": "showImg",
            "default": true
          },
          {
            "type": "uploadImage",
            "layoutType": "inline",
            "text": "图片地址",
            "bindKey": "topImgUrl",
            "placeholder": "可输入图片地址或直接上传"
          },
          {
            "type": "colorPicker",
            "layoutType": "inline",
            "text": "背景色",
            "bindKey": "colorBg",
            "placeholder": "请选择颜色"
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "累计积分",
            "bindKey": "titleFont"
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "积分",
            "bindKey": "subTitleFont"
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "描述积分",
            "bindKey": "descTitleFont"
          }
        ]
      },
      {
        "type": "group",
        "text": "底部容器样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "colorPicker",
            "layoutType": "inline",
            "text": "背景色",
            "bindKey": "bgColor",
            "placeholder": "请选择颜色",
            "default": {
              "hex": "#FFFFFFFF",
              "hex8": "#FFFFFFFF"
            }
          },
          {
            "type": "input",
            "layoutType": "inline",
            "text": "容器阴影",
            "bindKey": "boxShadow",
            "default": "0px 0px 10px 0px #e3dede"
          },
          {
            "type": "border",
            "layoutType": "block",
            "text": "容器边框",
            "bindKey": "boxBorder"
          },
          {
            "type": "box",
            "layoutType": "block",
            "text": "内外边距",
            "bindKey": "box",
            "default": {
              "marginTopVal": "-20px",
              "marginRightVal": "10px",
              "marginBottomVal": "",
              "marginLeftVal": "10px",
              "paddingTopVal": "",
              "paddingRightVal": "10px",
              "paddingBottomVal": "",
              "paddingLeftVal": "10px"
            }
          },
          {
            "type": "group",
            "text": "积分奖励样式",
            "desc": "点击？查看样式设置用法指南",
            "helpUrl": "",
            "children": [
              {
                "type": "switch",
                "layoutType": "inline",
                "text": "标题显示",
                "desc":"积分奖励是否显示",
                "bindKey": "integralShow",
                "default": true
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "积分奖励",
                "bindKey": "integralTitle",
                "default": "积分奖励"
              },
              {
                "type": "font",
                "layoutType": "block",
                "text": "积分字体",
                "bindKey": "integralFont"
              }
            ]
          },
          {

          }
        ]
      },
      {
        "type": "table",
        "text": "主题设置",
        "desc": "点击？查看主题设置用法指南",
        "bindKey": "themeList",
        "tabText": "主题[index]",
        "default":[
            {
                "key":"blue",
                "mainColor":{
                    "hex": "#0073CA",
                    "hex8": "#0073CAFF"
                }
            },
            {
                "key":"red",
                "mainColor":{
                    "hex": "#E21A1A",
                    "hex8": "#E21A1AFF"
                }
            },
            {
                "key":"green",
                "mainColor":{
                    "hex": "#0EAF64",
                    "hex8": "#0EAF64FF"
                }
            }
        ],
        "children": [
            {
                "type": "select",
                "layoutType": "inline",
                "text": "主题标识",
                "bindKey": "key",
                "multiple": false,
                "desc":"用于设置主题的标识，此项需要搭配自定义开发切换主题的功能，例如：蓝色标识为blue，用户切换主题的时候存储到数据库中的标识为blue，则这里填写为blue",
                "default":"",
                "placeholder":"例如：blue",
                "ctrlAttrObject": {
                    "mode": "combobox"
                },
                "dictionary": [
                    {
                        "label": "blue",
                        "value": "blue"
                    },
                    {
                        "label": "red",
                        "value": "red"
                    },
                    {
                        "label": "green",
                        "value": "green"
                    }
                ]
            },{
                "type": "colorPicker",
                "layoutType": "inline",
                "text": "主要颜色",
                "bindKey": "mainColor",
                "desc":"主题的主要颜色",
                "placeholder": "请选择颜色",
                "display":"@[key]"
            }
        ]
      },
      {
        "type": "group",
        "text": "高级",
        "desc": "点击？查看高级设置用法指南",
        "children": [
          {
            "type": "pageModuleSelect",
            "layoutType": "block",
            "text": "tab菜单联动组件",
            "desc": "选择点击tab需要联动的组件",
            "bindKey": "triggerComponents",
            "multiple": true
          }
        ]
      }
  ]
}